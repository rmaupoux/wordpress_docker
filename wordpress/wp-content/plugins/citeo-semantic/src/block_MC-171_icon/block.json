{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "citeo-semantic/icon",
	"version": "1.0.0",
	"title": "Icône",
	"category": "semantic",
	"icon": "smiley",
	"description": "Icône de la librairie Citeo",
	"example": {},
	"attributes": {
		"icon": {
			"type": "string",
			"default": "Aucun"
		},
