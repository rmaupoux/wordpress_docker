{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "citeo-semantic/progress-bar",
	"version": "1.0.0",
	"title": "Barre de progression",
	"category": "semantic",
	"icon": "smiley",
	"description": "Barre de progression pour statistique",
	"example": {},
	"attributes": {
		"progress": {
			"type": "number",
			"default": 0
		},
		"color": {
			"type": "string",
			"default": "#FACC15"
		}
	},
	"supports": {
		"html": false
	},
	"textdomain": "semantic",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
