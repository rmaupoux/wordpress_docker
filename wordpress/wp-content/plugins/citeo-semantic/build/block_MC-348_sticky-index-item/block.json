{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "citeo-semantic/sticky-index-item",
  "version": "1.0.0",
  "title": "Entrée du sommaire",
  "category": "semantic",
  "description": "Entrée du sommaire synchronisé avec le titre d'une section. Non éditable",
  "supports": {
    "html": false,
    "align": [
      "full"
    ],
    "inserter": false
  },
  "attributes": {
    "sectionTitle": {
      "type": "string",
      "default": ""
    },
    "uuid": {
      "type": "string"
    }
  },
  "example": {},
  "ancestor": [
    "citeo-semantic/sticky-index-page"
  ],
  "usesContext": [
    "citeo-semantic/section-wrapper-id"
  ],
  "textdomain": "citeo",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}