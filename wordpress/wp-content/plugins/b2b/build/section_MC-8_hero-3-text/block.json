{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "b2b/section-hero-text",
  "version": "1.0.0",
  "title": "En tête - Support",
  "category": "b2b-page-start-block",
  "description": "Permet d'avoir du texte sur un fond coloré",
  "supports": {
    "html": false,
    "align": [
      "full"
    ]
  },
  "attributes": {
    "allowResize": {
      "type": "boolean",
      "default": false
    },
    "preview": {
      "type": "boolean",
      "default": false
    },
    "align": {
      "type": "string",
      "default": "full"
    },
    "theme": {
      "type": "string",
      "default": ""
    },
    "subTheme": {
      "type": "string",
      "default": "subtheme-accent1"
    }
  },
  "example": {
    "attributes": {
      "preview": true
    }
  },
  "providesContext": {
    "allowResize": "allowResize"
  },
  "textdomain": "headelines",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}