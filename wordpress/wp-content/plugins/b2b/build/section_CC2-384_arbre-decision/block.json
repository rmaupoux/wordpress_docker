{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "ds-citeocom/section-chart",
  "version": "2.0.0",
  "title": "Arbre de décision",
  "category": "b2b-page-builder-block",
  "description": "Ce bloc permet de mettre en avant un arbre de décision",
  "supports": {
    "html": false,
    "align": [
      "full"
    ]
  },
  "attributes": {
    "preview": {
      "type": "boolean",
      "default": false
    },
    "align": {
      "type": "string",
      "default": "full"
    },
    "theme": {
      "type": "string",
      "default": ""
    },
    "subTheme": {
      "type": "string",
      "default": "subtheme-accent1"
    },
    "themeSwapConfirm": {
      "type": "boolean",
      "default": false
    },
    "backgroundPictoUrl": {
      "type": "string",
      "default": ""
    },
    "isWhite": {
      "type": "boolean",
      "default": false
    }
  },
  "example": {
    "attributes": {
      "preview": true
    }
  },
  "allowedBlocks": [
    ""
  ],
  "textdomain": "headelines",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./js/view.js"
}