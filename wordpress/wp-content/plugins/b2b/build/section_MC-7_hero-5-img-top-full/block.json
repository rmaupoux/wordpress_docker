{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "b2b/hero-img-full-text-above",
  "version": "1.0.1",
  "title": "En tête - Froid",
  "category": "b2b-page-start-block",
  "description": "Ce bloc permet de mettre lumière l'accroche / le pourquoi de la page",
  "supports": {
    "html": false,
    "align": [
      "full"
    ]
  },
  "attributes": {
    "preview": {
      "type": "boolean",
      "default": false
    },
    "heroImgPosition": {
      "type": "string",
      "default": "hero-img-center"
    },
    "align": {
      "type": "string",
      "default": "full"
    },
    "theme": {
      "type": "string",
      "default": ""
    },
    "subTheme": {
      "type": "string",
      "default": "subtheme-accent1"
    }
  },
  "example": {
    "attributes": {
      "preview": true
    }
  },
  "textdomain": "headelines",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}