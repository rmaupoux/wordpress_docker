{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "b2b/section-jobs-to-be-done",
	"version": "1.0.0",
	"title": "Jobs to be done - Texte",
	"category": "b2b-service-block",
	"icon": "smiley",
	"description": "Permet de présenter plusieurs liens vers des ressources internes ou externes à la page à travers des applats de texte.",
	"supports": {
		"html": false
	},
	"attributes": {
		"allowResize": {
			"type": "boolean",
			"default": false
		},
		"preview": {
			"type": "boolean",
			"default": false
    	},
		"textSize": {
			"type": "string",
			"default": "big-cta"
		}
	},
	"example": {
		"attributes": {
			"preview": true
		}
	},
	"providesContext": {
		"allowResize": "allowResize"
	},
	"textdomain": "citeo",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}