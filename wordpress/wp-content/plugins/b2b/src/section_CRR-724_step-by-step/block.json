{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "b2b/section-step-by-step",
	"version": "1.0.0",
	"title": "Étape par étape",
	"category": "b2b-page-builder-block",
	"description": "Ce bloc permet d'afficher des slides numérotées avec un titre, une description et une photo",
	"supports": {
		"html": false
	},
	"attributes": {
		"preview": {
			"type": "boolean",
			"default": false
		},
		"theme": {
			"type": "string",
			"default": ""
		},
		"subTheme": {
			"type": "string",
			"default": "subtheme-accent1"
		},
		"themeSwapConfirm": {
            "type": "boolean",
            "default": false
        }
	},
	"example": {
		"attributes": {
			"preview": true
		}
	},
	"textdomain": "citeo",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./js/view.js"
}