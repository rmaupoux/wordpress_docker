	"usesContext": ["b2b/theme-context"],
	"ancestor": ["ds-citeocom/section-vignettes", "ds-citeocom/section-box-media"],
	"textdomain": "citeo",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
