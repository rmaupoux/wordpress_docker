{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "ds-citeocom/section-article",
	"version": "2.0.0",
	"title": "Texte & image",
	"category": "b2b-page-builder-block",
	"description": "Bloc permettant de pr√©senter d'autres pages internes.",
	"supports": {
		"html": false,
		"align": [
			"full"
		]
	},
	"attributes": {
		"preview": {
			"type": "boolean",
			"default": false
		},
		"theme": {
            "type": "string",
            "default": ""
        },
		"subTheme": {
            "type": "string",
            "default": "subtheme-accent1"
        },
        "themeSwapConfirm": {
            "type": "boolean",
            "default": false
        }
	},
	"example": {
		"attributes": {
			"preview": true
		}
	},
	"textdomain": "headelines",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}